   clock-names = "bus", "core";
   power-domains = <&pd_pu>;
  };

  timer@00a00600 {
   compatible = "arm,cortex-a9-twd-timer";
   reg = <0x00a00600 0x20>;
   interrupts = <1 13 0xf01>;
   interrupt-parent = <&intc>;
   clocks = <&clks 15>;
  };

  intc: interrupt-controller@00a01000 {
   compatible = "arm,cortex-a9-gic";
   #interrupt-cells = <3>;
